<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.springGroupS.dao.StudyDAO">
	<select id="getUserList" resultType="com.spring.springGroupS.vo.UserVO">
		SELECT * FROM user;
	</select>
	<select id="getUser" parameterType="String" resultType="com.spring.springGroupS.vo.UserVO">
		SELECT * FROM user WHERE mid = #{mid} LIMIT 1;
	</select>
	<select id="getUserListLike" parameterType="String" resultType="com.spring.springGroupS.vo.UserVO">
		SELECT * FROM user WHERE mid like concat('%',#{mid},'%');
	</select>
</mapper>